    "babel-loader": "^8.3.0",
    "chromatic": "^6.15.0",
    "eslint": "^8.33.0",
    "eslint-config-smarthr": "^6.7.0",
    "eslint-plugin-storybook": "^0.6.10",
    "msw": "^1.0.0",
    "msw-storybook-addon": "^1.7.0",
    "prettier": "^2.8.3",
    "storybook": "^7.0.0-rc.4",
    "typescript": "^4.9.3",
    "vite": "^4.0.0",
    "wait-on": "^7.0.1"
  },
  "msw": {
    "workerDirectory": "public"
  }
